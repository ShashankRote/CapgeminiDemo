{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "playbookadfv2"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/DS_Blobzip')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LS_blobstorage",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlob",
				"structure": [
					{
						"name": "Year",
						"type": "String"
					},
					{
						"name": "Month",
						"type": "String"
					},
					{
						"name": "Geomarket",
						"type": "String"
					},
					{
						"name": "Sub Geomarket",
						"type": "String"
					},
					{
						"name": "Product Line",
						"type": "String"
					},
					{
						"name": "Sub Product Line",
						"type": "String"
					},
					{
						"name": "GL Account Type",
						"type": "String"
					},
					{
						"name": "Local Curr",
						"type": "String"
					},
					{
						"name": "Local Amount",
						"type": "String"
					},
					{
						"name": "Transaction Curr",
						"type": "String"
					},
					{
						"name": "Transaction Amount",
						"type": "String"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": "\t",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"encodingName": null,
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": true
					},
					"compression": {
						"type": "ZipDeflate",
						"level": "Optimal"
					},
					"fileName": "STG_SAPECC_TRANS_2017_JAN.zip",
					"folderPath": "data"
				}
			}
		}
	]
}